// Setup event listeners
function setupEventListeners() {
    // Search functionality with debouncing and loading state
    function setupSearch() {
        const searchInput = document.getElementById('new-search');
        const searchButton = document.getElementById('new-search-btn');
        const searchRefurbishedInput = document.getElementById('refurbished-search');
        const searchRefurbishedButton = document.getElementById('refurbished-search-btn');
        
        // Show loading state
        const showLoading = (container) => {
            const loading = document.createElement('div');
            loading.className = 'loading-state';
            loading.innerHTML = '<div class="spinner"></div><p>Searching...</p>';
            container.appendChild(loading);
            return loading;
        };
        
        // Perform search with debouncing
        const performSearch = (searchTerm, containerId) => {
            const container = document.querySelector(containerId);
            const loading = showLoading(container);
            
            // Use requestAnimationFrame to ensure UI updates
            requestAnimationFrame(() => {
                const rows = container.querySelectorAll('tbody tr');
                let visibleCount = 0;
                
                rows.forEach(row => {
                    const text = row.textContent.toLowerCase();
                    const isVisible = searchTerm === '' || text.includes(searchTerm);
                    row.style.display = isVisible ? '' : 'none';
                    if (isVisible) visibleCount++;
                });
                
                // Show no results message if needed
                const noResults = container.querySelector('.no-results');
                if (visibleCount === 0 && searchTerm !== '') {
                    if (!noResults) {
                        const message = document.createElement('tr');
                        message.className = 'no-results';
                        message.innerHTML = `<td colspan="11" style="text-align: center; padding: 2rem;">No laptops found matching "${searchTerm}"</td>`;
                        container.querySelector('tbody').appendChild(message);
                    }
                } else if (noResults) {
                    noResults.remove();
                }
            };
            
            // Debounced search functions
            const debouncedSearch = debounce((e) => {
                performSearch(e.target.value.toLowerCase(), '#new-laptops .table-container');
            }, 300);
            
            const debouncedSearchRefurbished = debounce((e) => {
                performSearch(e.target.value.toLowerCase(), '#refurbished .table-container');
            }, 300);
            
            // Event listeners for search
            if (searchInput) {
                searchInput.addEventListener('input', debouncedSearch);
                searchInput.addEventListener('search', (e) => {
                    if (e.target.value === '') performSearch('', '#new-laptops .table-container');
                });
            }
            
            if (searchButton) {
                searchButton.addEventListener('click', () => {
                    performSearch(searchInput.value.toLowerCase(), '#new-laptops .table-container');
                });
            }
            
            if (searchRefurbishedInput) {
                searchRefurbishedInput.addEventListener('input', debouncedSearchRefurbished);
                searchRefurbishedInput.addEventListener('search', (e) => {
                    if (e.target.value === '') performSearch('', '#refurbished .table-container');
                });
            }
            
            if (searchRefurbishedButton) {
                searchRefurbishedButton.addEventListener('click', () => {
                    performSearch(searchRefurbishedInput.value.toLowerCase(), '#refurbished .table-container');
                });
            }
        }
        
        setupSearch();
    }
    
    // Initialize the application with performance optimizations
    function init() {
        // Show loading state
        const mainContent = document.querySelector('main');
        const loadingOverlay = document.createElement('div');
        loadingOverlay.className = 'loading-overlay';
